elm = $("#js-saved-<%= registrant.id %>")
elm.addClass('fade-in-save');
trueelm = elm[0]
if(trueelm) {
    var newone = trueelm.cloneNode(true);
    trueelm.parentNode.replaceChild(newone, trueelm);
}
var checkedIn = $("#js-checkedIn")[0];
if (checkedIn) {
    checkedIn.checked = <%= registrant.checked_in? %>;
}
